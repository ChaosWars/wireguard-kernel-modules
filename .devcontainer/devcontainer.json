{
	"customizations": {
		"vscode": {
			"extensions": [
				"GitHub.vscode-github-actions",
                "Guyutongxue.cpp-reference",
                "GitHub.vscode-pull-request-github",
                "GitHub.remotehub",
				"ms-azuretools.vscode-docker",
				"ms-python.python",
				"ms-vscode.cpptools",
				"ms-vscode.makefile-tools"
			]
		}
	},
	"dockerComposeFile": [
		"docker-compose.yml"
	],
	"features": {
		"ghcr.io/devcontainers-contrib/features/apt-get-packages:1": {
			"packages": "kmod,sudo"
		},
		"ghcr.io/devcontainers-contrib/features/zsh-plugins:0": {},
		"ghcr.io/devcontainers/features/docker-outside-of-docker": {},
		"ghcr.io/stuartleeks/dev-container-features/shell-history": {}
	},
	"initializeCommand": ".devcontainer/init.sh",
	"overrideCommand": true,
	"postAttachCommand": ".devcontainer/postAttach.sh",
	"remoteUser": "root",
	"service": "wireguard-kernel-modules",
	"workspaceFolder": "/workspace/wireguard-kernel-modules"
}
